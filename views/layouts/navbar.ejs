    <body>
        
        <nav>
        <a href="/" class="logo">BEAUTYSPAR</a>
        <ul class="nav-menu">
           
                <% if(typeof navigations != 'undefined'){ %> 
                    <% navigations.forEach(nav =>{ %>
                        <li class="nav-item <%= (typeof activeNav != 'undefined' && activeNav == nav.active)?'nav-active':'' %> ">
                            <a href="<%= nav.href %> "><%= nav.name %> </a></li>
                   <% }) %> 
              <%  } %>  
            <!-- <li class="nav-item"><a href="/failedBookings" >FailedBookings</a> </li> -->
           
          
           
        </ul>
    </nav> 

    
